{
  "name": "Liam Security",
  "version": "0.0.1",
  "background": {
    "scripts": ["eventPage.js"],
    "persistenet": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "<all_urls>",
    "webNavigation"
  ]
}
